<ul class="collection with-header">
    <li class="collection-header">
        <h5>Visibilité</h5>
    </li>
    <li class="collection-item">
        <div>
            <strong>Journal</strong>
            <a href="#!" class="secondary-content btn-visibility" data-type="book">
                {% if true == bookVisibility %}
                <span class="public-btn">Publique</span>
                {% else %}
                <span class="private-btn">Privé</span>
                {% endif %}
            </a>
        </div>
    </li>
    <li class="collection-item">
        <div>
            <strong>Dates</strong>
            <a href="#!" class="secondary-content btn-visibility" data-type="date">
                {% if true == datesVisibility %}
                <span class="public-btn">Publique</span>
                {% else %}
                <span class="private-btn">Privé</span>
                {% endif %}
            </a>
        </div>
    </li>
    <li class="collection-item">
        <div>Alvin<a href="#!" class="secondary-content"><i class="material-icons">send</i></a></div>
    </li>
    <li class="collection-item">
        <div>Alvin<a href="#!" class="secondary-content"><i class="material-icons">send</i></a></div>
    </li>
</ul>